<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <security sessionManager="Supido.Demo.Service.Security.SessionManager" securityManager="Supido.Demo.Service.Security.SecurityManager"></security>
  <service cors="true" apiPath="api">
    <api path="client" entityType="Supido.Demo.Model.Client" parameterName="ClientId">
      <api path="project" entityType="Supido.Demo.Model.Project" parameterName="ProjectId">
        <api path="service" entityType="Supido.Demo.Model.Service" parameterName="ServiceId">
          <api path="task" entityType="Supido.Demo.Model.Task" parameterName="TaskId"></api>
        </api>
      </api>
    </api>
    <api path="department" entityType="Supido.Demo.Model.Department" parameterName="DepartmentId"></api>
  </service>
</configuration>
